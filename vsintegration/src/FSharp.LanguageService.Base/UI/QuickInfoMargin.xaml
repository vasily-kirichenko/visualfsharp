<UserControl x:Class="FSharp.LanguageService.Base.UI.QuickInfoMargin"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FSharp.LanguageService.Base.UI"
             mc:Ignorable="d" 
             Height="24"
             d:DesignHeight="25" d:DesignWidth="600">
    <TextBox x:Name="tbQuickInfo" 
        IsEnabled="True" 
        IsReadOnly="True" 
        Text="{Binding QuickInfo, Mode=OneWay}" />
</UserControl>